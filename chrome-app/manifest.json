{
  "manifest_version": 2,
  "name": "Boustrophedon",
  "description": "Boustrophedon in your browser.",
  "version": "0.1",
  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["jquery-2.1.0.min.js", "boustrophedon.js"],
    "css": ["boustrophedon.css"]
  }],
  "browser_action": {
    "default_icon": "calculator-16.png"
  },
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" }
}
